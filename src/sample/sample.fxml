<?import javafx.scene.layout.BorderPane?>
<?import sample.Unit?>

<?import sample.Player?>
<?import sample.Enemy?>
<BorderPane
        fx:id="pane"
        fx:controller="sample.Controller"
        xmlns:fx="http://javafx.com/fxml"
        onKeyPressed="#handleKeyPressed"
        onKeyReleased="#handleKeyReleased"
>
    <Player
            fx:id="player"
            centerX="50"
            centerY="50"
            strokeWidth="1.5"
            stroke="#1f9ec5"
            fill="#47b5e4"
            radius="20"
    >

    </Player>

    <Enemy
            fx:id="enemy"
            centerX="80"
            centerY="40"
            strokeWidth="1.5"
            stroke="#cb0800"
            fill="#e42f35"
            radius="25">
    </Enemy>

</BorderPane>